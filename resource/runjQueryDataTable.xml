<?xml version="1.0" encoding="UTF-8"?>
<suite name="DataTable">

    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
        <listener class-name="commons.MethodListener"/>
        <listener class-name="reportConfig.ExtentTestListener"/>
        <listener class-name="reportConfig.ReportNG"/>
    </listeners>

    <parameter name="url" value="https://www.jqueryscript.net/demo/jQuery-Dynamic-Data-Grid-Plugin-appendGrid/" />

    <test name="Run on Chrome">
        <parameter name="browser" value = "chrome"/>
        <classes>
                <class name="com.jQuery.User_01_DataTable_DataGrid"/>
        </classes>
    </test>
</suite>